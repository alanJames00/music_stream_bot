#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/youtube-exec@1.0.4/node_modules/youtube-exec/bin/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/youtube-exec@1.0.4/node_modules/youtube-exec/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/youtube-exec@1.0.4/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/youtube-exec@1.0.4/node_modules/youtube-exec/bin/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/youtube-exec@1.0.4/node_modules/youtube-exec/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/youtube-exec@1.0.4/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../youtube-exec/bin/youtube-exec.js" "$@"
else
  exec node  "$basedir/../youtube-exec/bin/youtube-exec.js" "$@"
fi
