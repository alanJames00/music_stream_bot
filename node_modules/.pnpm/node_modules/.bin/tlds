#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/tlds@1.244.0/node_modules/tlds/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/tlds@1.244.0/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/tlds@1.244.0/node_modules/tlds/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/tlds@1.244.0/node_modules:/home/alanjames/development/music_streamer_v2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tlds/bin.js" "$@"
else
  exec node  "$basedir/../tlds/bin.js" "$@"
fi
